pipeline{
    agent any
    stages{
        stage('Inicio'){
            steps{
                bat 'echo Deu certo!'
            }
        }

        stage('Maven'){
            steps{
                bat 'mvn -version'
            }
        }

        stage('Navegando Dir'){
            steps{
                dir("src/br/com/programa"){
                    stage('TESTE'){
                        steps{
                            bat 'java Programa.java'
                        }
                    }
                    

                    bat 'dir'
                }
             }
         }

         stage('Executando programa'){
             steps{
                 bat 'java Programa.java'
             }
         }
    }
}
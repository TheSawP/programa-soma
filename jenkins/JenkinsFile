pipeline{
    agent any
    stages{
        stage('Inicio'){
            steps{
                bat 'echo Deu certo!'
            }
        }

        stage('Maven'){
            steps{
                bat 'mvn -version'
            }
        }

        stage('Navegando Dir'){
            steps{
                dir("src/br/com/programa"){

                        bat 'java Programa.java ola'                   
                                      
                }
             }
         }

         stage('Executando programa'){
             steps{
                 bat 'java Programa.java'
             }
         }
    }
}
pipeline{
    agent any
    stages{
        stage('Inicio'){
            steps{
                bat 'echo Deu certo!'
            }
        }

        stage('Maven'){
            steps{
                bat 'mvn -version'
            }
        }

        stage('Navegando Dir'){
            steps{
                bat 'cd src'
                bat 'cd br'
                bat 'cd com'
                bat 'cd programa'
            }
        }
    }
}